    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.0.0",
    "lucide-react": "^0.503.0",
    "prettier": "3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
